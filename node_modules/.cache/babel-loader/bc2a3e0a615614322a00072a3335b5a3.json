ACA,QAAA,KAAA,CAAA,OAAA,GAAA,IAAA,CAAA,OAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,IAAA,CAAA,OAAA;AACA,QAAA,KAAA,CAAA,QAAA,GAAA,IAAA,CAAA,IAAA;AACA,OATA,EASA,KATA,CASA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAXA;AAYA,KAfA;AAgBA,IAAA,UAhBA,wBAgBA;AACA,MAAA,QAAA,CAAA,UAAA;AACA,KAlBA;AAmBA,IAAA,aAnBA,2BAmBA;AACA,MAAA,QAAA,CAAA,aAAA;AACA,KArBA;AAsBA,IAAA,UAtBA,sBAsBA,EAtBA,EAsBA;AAAA;;AACA,WAAA,QAAA,CAAA,UAAA,EAAA,IAAA,EAAA;AACA,QAAA,kBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA,IAFA;AAGA,QAAA,IAAA,EAAA;AAHA,OAAA,EAKA,IALA,CAKA,YAAA;AACA,QAAA,QAAA,CAAA,aAAA,CAAA,EAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,UAAA,MAAA,CAAA,cAAA,CAAA,CAAA;;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,QAAA;;AACA,UAAA,MAAA,CAAA,QAAA,CAAA;AACA,YAAA,IAAA,EAAA,SADA;AAEA,YAAA,OAAA,EAAA;AAFA,WAAA;AAIA,SARA,EAQA,KARA,CAQA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,SAVA;AAWA,OAjBA,EAkBA,KAlBA,CAkBA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OApBA;AAqBA,KA5CA;AA6CA;AACA,IAAA,gBA9CA,4BA8CA,QA9CA,EA8CA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,cAAA,CAAA,KAAA,WAAA;AACA,KAjDA;AAkDA,IAAA,mBAlDA,+BAkDA,WAlDA,EAkDA;AACA,WAAA,WAAA,GAAA,WAAA;AACA,WAAA,cAAA,CAAA,KAAA,WAAA;AACA;AArDA,GArBA;AA4EA,EAAA,QAAA,EAAA;AA5EA,CAAA","sourcesContent":["<template>\r\n    <div class=\"\">\r\n\r\n        <!-- 添加记录 -->\r\n        <el-button type=\"text\" @click=\"dialogFormVisible = true\">添加记录</el-button>\r\n\r\n        <el-dialog title=\"收货地址\" :visible.sync=\"dialogFormVisible\">\r\n            <el-form :model=\"form\">\r\n                <el-form-item label=\"播报日期\" label-width=\"120px\">\r\n                    <el-input v-model=\"festivalBroadcast.broadcastDay\" autocomplete=\"off\"></el-input>\r\n                </el-form-item>\r\n                <el-form-item label=\"时间戳\" label-width=\"120px\">\r\n                    <el-select v-model=\"festivalBroadcast.timestamp\" placeholder=\"请选择活动区域\">\r\n                        <el-option label=\"区域一\" value=\"shanghai\"></el-option>\r\n                        <el-option label=\"区域二\" value=\"beijing\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n\r\n            </el-form>\r\n            <div slot=\"footer\" class=\"dialog-footer\">\r\n                <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\r\n                <el-button type=\"primary\" @click=\"dialogFormVisible = false\">确 定</el-button>\r\n            </div>\r\n        </el-dialog>\r\n\r\n        <br>\r\n\r\n        <!-- 展示记录 -->\r\n        <el-table :data=\"records\"\r\n                  style=\"width: 100%\">\r\n            <el-table-column type=\"expand\">\r\n                <template slot-scope=\"props\">\r\n                    <el-form label-position=\"left\" inline class=\"demo-table-expand\">\r\n                        <el-form-item label=\"id\">\r\n                            <span>{{props.row.id}}</span>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"播报日期\">\r\n                            <span>{{props.row.broadcastDay}}</span>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"文件名\">\r\n                            <span>{{props.row.name}}</span>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"链接地址\">\r\n                            <span>{{props.row.url}}</span>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"文件大小\">\r\n                            <span>{{props.row.size}}</span>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"MD5值\">\r\n                            <span>{{props.row.md5}}</span>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"时间戳\">\r\n                            <span>{{props.row.timestamp}}</span>\r\n                        </el-form-item>\r\n                        <el-form-item label=\"创建时间\">\r\n                            <span>{{props.row.createTime}}</span>\r\n                        </el-form-item>\r\n                    </el-form>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column fixed type=\"index\"/>\r\n            <el-table-column label=\"播报日期\" prop=\"broadcastDay\"/>\r\n            <el-table-column label=\"文件名\" prop=\"name\"/>\r\n            <el-table-column label=\"创建时间\" prop=\"createTime\"/>\r\n            <el-table-column fixed=\"right\" label=\"操作\">\r\n                <template slot-scope=\"scope\">\r\n                    <el-button @click=\"updateHoliday()\" type=\"text\" size=\"small\">编辑</el-button>\r\n                    <el-button @click=\"deleteById(scope.row.id)\" type=\"text\" size=\"small\">删除</el-button>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n\r\n        <br>\r\n\r\n        <!-- 分页功能 -->\r\n        <span class=\"demonstration\"></span>\r\n        <el-pagination\r\n                @size-change=\"handleSizeChange\"\r\n                @current-change=\"handleCurrentChange\"\r\n                :current-page=\"pageCurrent\"\r\n                :page-sizes=\"[10,20,50,100]\"\r\n                :page-size=\"pageSize\"\r\n                layout=\"total, sizes, prev, pager, next, jumper\"\r\n                :total=\"total\">\r\n        </el-pagination>\r\n    </div>\r\n\r\n</template>\r\n\r\n<style>\r\n    .demo-table-expand {\r\n        font-size: 0;\r\n    }\r\n\r\n    .demo-table-expand label {\r\n        width: 90px;\r\n        color: #99a9bf;\r\n    }\r\n\r\n    .demo-table-expand .el-form-item {\r\n        margin-right: 0;\r\n        margin-bottom: 0;\r\n        width: 100%;\r\n    }\r\n</style>\r\n\r\n<script>\r\n    import festival from '../../api/application/festival'\r\n\r\n    export default {\r\n        name: 'FestivalList',\r\n        data() {\r\n            return ({\r\n                total: 0,\r\n                pages: 0,\r\n                records: [],\r\n                pageCurrent: 1,\r\n                pageSize: 10,\r\n                festivalBroadcast: {\r\n                    broadcastDay: \"\",\r\n                    url: \"\",\r\n                    timestamp: null,\r\n                    md5: null,\r\n                    size: null,\r\n                }\r\n            })\r\n        },\r\n        created() {\r\n            this.getHolidayList(1)\r\n        },\r\n        methods: {\r\n            getHolidayList(pageCurrent) {\r\n                this.pageCurrent = pageCurrent;\r\n                festival.getHolidayList(this.pageCurrent, this.pageSize)\r\n                    .then(response => {\r\n                        console.log(response);\r\n                        let data = response.data;\r\n                        this.total = data.total;\r\n                        this.pages = data.pages;\r\n                        this.records = data.records;\r\n                        this.pageCurrent = data.current;\r\n                        this.pageSize = data.size;\r\n                    }).catch(error => {\r\n                    console.log(error)\r\n                })\r\n            },\r\n            addHoliday() {\r\n                festival.addHoliday()\r\n            },\r\n            updateHoliday() {\r\n                festival.updateHoliday()\r\n            },\r\n            deleteById(id) {\r\n                this.$confirm('确认删除该记录？', '提示', {\r\n                    confirmButtonClass: '确定',\r\n                    cancelButtonText: '取消',\r\n                    type: 'warning'\r\n                })\r\n                    .then(() => {\r\n                        festival.deleteHoliday(id)\r\n                            .then(response => {\r\n                                this.getHolidayList(1);\r\n                                console.log(response);\r\n                                this.$message({\r\n                                    type: 'success',\r\n                                    message: '删除成功!'\r\n                                })\r\n                            }).catch(error => {\r\n                            console.log(error)\r\n                        });\r\n                    })\r\n                    .catch(error => {\r\n                        console.log(error)\r\n                    });\r\n            },\r\n            //分页功能\r\n            handleSizeChange(pageSize) {\r\n                this.pageSize = pageSize;\r\n                this.getHolidayList(this.pageCurrent)\r\n            },\r\n            handleCurrentChange(pageCurrent) {\r\n                this.pageCurrent = pageCurrent;\r\n                this.getHolidayList(this.pageCurrent)\r\n            }\r\n        },\r\n        computed: {}\r\n    }\r\n</script>\r\n"],"sourceRoot":"src/views/application"}]}